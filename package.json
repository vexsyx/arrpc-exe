{
  "name": "arrpc-exe",
  "version": "3.6.0",
  "description": "A standalone EXE allowing people to use Web Discord RPC (arRPC) without having to install Node.",
  "main": "src/server.js",
  "scripts": {
    "start": "node src",
    "build": "pkg --no-bytecode ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vexsyx/arrpc-exe.git"
  },
  "author": "OpenAsar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenAsar/arrpc/issues"
  },
  "homepage": "https://github.com/OpenAsar/arrpc#readme",
  "dependencies": {
    "pkg": "^5.8.1",
    "ws": "^8.18.3"
  },
  "type": "commonjs",
  "bin": {
    "arrpc-exe": "src/index.js"
  },
  "pkg": {
    "assets": [ "src/process/detectable.json" ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  }
}
